{
  "name": "wind-waker",
  "version": "0.0.0-semantically-released",
  "main": "lib/index.js",
  "types": "lib/types/index.d.ts",
  "repository": "git@github.com:Cervantes007/ww.git",
  "description": "Nodejs framework to build high-performance app in no time",
  "keywords": [
    "node",
    "nodejs",
    "framework",
    "typescript",
    "fast",
    "blazing",
    "performance",
    "decorator"
  ],
  "author": "Cervantes007",
  "engines": {
    "node": ">=12.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "license": "MIT",
  "scripts": {
    "api": "ts-node-dev --respawn example/index.ts",
    "build": "tsc",
    "docs": "yarn docs:generate && yarn docs:build",
    "docs:start": "yarn docs:generate && vuepress dev docs",
    "docs:generate": "rimraf docs && typedoc && cp -r vuepress/* docs/ && cp -r vuepress/.vuepress docs",
    "docs:dev": "vuepress dev vuepress",
    "docs:cp": "cp -r vuepress/* docs/",
    "docs:build": "vuepress build docs",
    "docs:serve": "yarn docs && serve docs/.vuepress/dist",
    "lint": "eslint --ignore-pattern '/lib/' '*/**/*.ts' --quiet --fix"
  },
  "devDependencies": {
    "@types/co-body": "^5.1.0",
    "@types/cors": "^2.8.9",
    "@types/node": "^14.14.22",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.2.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.4.2",
    "ts-node-dev": "^1.1.1",
    "typedoc": "^0.20.23",
    "typedoc-plugin-markdown": "^3.4.5",
    "typescript": "^4.1.3",
    "vuepress": "^1.8.0",
    "vuepress-plugin-one-click-copy": "^1.0.2",
    "vuepress-plugin-typedoc": "^0.4.0"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "co-body": "6.1.0",
    "cors": "2.8.5",
    "glob": "^7.1.6",
    "helmet": "4.4.1",
    "reflect-metadata": "^0.1.13"
  }
}
